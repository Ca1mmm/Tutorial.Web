@model Tutorial.Web.Model.Student
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detail</title>
</head>
<body>
    <div>
        <h1>学生信息：</h1>
        <div>ID:@Model.Id</div>
        <div>姓名:@Model.FirstName @Model.LastName</div>
        <div>性别:@Model.Gender</div>
        <div>出生日期:@Model.BirthDay.ToString("yyyy-MM-dd")</div>
    </div>
    <a asp-action="Index">返回 Home</a>
</body>
</html>
